{% extends 'base.html.twig' %}

{% block body %}

    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            <h1>
                {{ title }}
            </h1>
            {% for message in app.flashes('db_error') %}
                <div class="alert alert-danger" role="alert">
                    {{ message }}
                </div>
            {% endfor %}
        </div>
    </div>

{% endblock %}